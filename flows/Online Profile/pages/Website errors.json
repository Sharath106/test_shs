    } ],
    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.repeat_counter = null OR $session.params.repeat_counter = 3",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Laissez-moi vous aider avec les problèmes liés au site Web." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Pour résoudre la majorité des erreurs de site Internet et de navigateur, vous pouvez essayer ces trucs afin de résoudre le problème:" ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "S'il s'agit de Mon TELUS, essayez de vous déconnecter et de vous reconnecter." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Essayez de redémarrer votre navigateur." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Essayez d'utiliser un autre navigateur comme Google Chrome, Safari, Firefox ou Internet Explorer et vérifier si la même chose se produit." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Et puis videz la cache et les cookies de votre navigateur." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "I can help you with website related issues." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "To resolve a majority of website and browser errors, please try some of the following:" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "1. If it's relating to My TELUS, try logging out and back in." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "2. Try restarting your browser to refresh processes in the background." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "3. Try using a different browser like Google Chrome, Safari, Firefox or Internet Explorer to see if the same errors occur there." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "and 4. Clear your browser's cache and cookies" ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 1 OR $session.params.repeat_counter = 4",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Si vous rencontrez des problèmes liés au site web, je suis là pour vous aider. Pour résoudre la plupart des erreurs de site web et de navigateur, veuillez essayer les étapes suivantes :" ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Déconnectez-vous et reconnectez-vous si le problème est lié à Mon TELUS." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Redémarrez votre navigateur pour rafraîchir les processus en arrière-plan. Essayez d'utiliser un navigateur différent tel que Google Chrome, Safari, Firefox ou Internet Explorer pour vérifier si la même erreur se produit." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Et puis effacez le cache et les cookies de votre navigateur." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you're experiencing any website-related issues, I'm here to assist you. To resolve most website and browser errors, please try the following steps:" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "1. Log out and back in if the issue is related to My TELUS." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "2. Restart your browser to refresh background processes." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "3. Try using a different browser such as Google Chrome, Safari, Firefox, or Internet Explorer to check if the same error occurs." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "and 4. Clear your browser's cache and cookies." ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 2 OR $session.params.repeat_counter = 5",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Pour toute préoccupation liée au site web, je suis disponible pour vous aider. Pour résoudre la majorité des erreurs de site web et de navigateur, veuillez considérer les étapes suivantes :" ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si vous rencontrez des problèmes avec Mon TELUS, essayez de vous déconnecter et de vous reconnecter." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Redémarrez votre navigateur pour rafraîchir les processus en arrière-plan." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Expérimentez avec différents navigateurs, tels que Google Chrome, Safari, Firefox ou Internet" ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Explorer, pour voir si les mêmes problèmes se posent." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Et puis effacez le cache et les cookies de votre navigateur." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "For any website-related concerns, I'm available to help you. To address the majority of website and browser errors, consider the following steps:" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "1. If you're experiencing issues with My TELUS, try logging out and back in." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "2. Restart your browser to refresh the background processes." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "3. Experiment with different browsers, such as Google Chrome, Safari, Firefox, or Internet Explorer, to see if the same problems arise." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "and 4. Clear your browser's cache and cookies." ]
            },
            "languageCode": "en"
          }
        } ]
      } ]
