  "form": {
    "parameters": [ {
      "displayName": "dtmf_troubleshoot_vague",
      "required": true,
      "entityType": "@dtmf-menu-1-2-3-0",
      "fillBehavior": {
        "initialPromptFulfillment": {
          "conditionalCases": [ {
            "cases": [ {
              "condition": "$session.params.loop_counter = 1",
              "caseContent": [ {
                "message": {
                  "text": {
                    "text": [ "En quelques mots, dites-moi quel problème vous rencontrez." ]
                  },
                  "languageCode": "fr-ca"
                }
              }, {
                "message": {
                  "text": {
                    "text": [ "In a few words, please tell me what issue you're experiencing." ]
                  },
                  "languageCode": "en"
                }
              } ]
            }, {
              "condition": "$session.params.loop_counter = 2",
              "caseContent": [ {
                "message": {
                  "text": {
                    "text": [ "Je comprends que quelque chose ne fonctionne pas, mais pour vous aider, j'ai besoin d'en savoir un peu plus. Pouvez-vous m'aider à comprendre ce qui ne fonctionne pas exactement?" ]
                  },
                  "languageCode": "fr-ca"
                }
              }, {
                "message": {
                  "text": {
                    "text": [ "I understand something isn't working, but to help you out here, I'll need to know a bit more. Can you help me understand what exactly isn't working for you?" ]
                  },
                  "languageCode": "en"
                }
              } ]
            }, {
              "condition": "$session.params.loop_counter = 3",
              "caseContent": [ {
                "message": {
                  "text": {
                    "text": [ "J'ai besoin de plus de détails. Vous pouvez dire quelque chose comme \"problème d'appareil\", \"appels téléphoniques ne fonctionnent pas\", \"données ne fonctionnent pas\", \"problème avec ma messagerie vocale\"." ]
                  },
                  "languageCode": "fr-ca"
                }
              }, {
                "message": {
                  "text": {
                    "text": [ "I'll need more details here. You can say something like \"device issues\", \"phone calls not working\", \"data not working\", \"issues with my voicemail\"." ]
                  },
                  "languageCode": "en"
                }
              } ]
            } ]
          } ]
        },
        "repromptEventHandlers": [ {
          "event": "sys.no-match-1",
          "triggerFulfillment": {
            "messages": [ {
              "text": {
                "text": [ "Désolé, je ne pense pas avoir compris.", "Désolé, je n'ai pas tout à fait compris.", "Je ne crois pas avoir bien saisi." ]
              },
              "languageCode": "fr-ca"
            }, {
              "text": {
                "text": [ "Veuillez expliquer plus en détail quel problème vous rencontrez. Par exemple, je peux vous aider à résoudre vos problèmes d'appel, de mot de passe ou de téléphone qui ne fonctionne pas." ]
              },
              "languageCode": "fr-ca"
            }, {
              "text": {
                "text": [ "Sorry, I didn't catch that.", "I'm sorry, I didn't understand that", "Sorry, I didn't get that." ]
              },
              "languageCode": "en"
            }, {
              "text": {
                "text": [ "Please explain in more detail what issue you're facing. For example, I can help you with troubleshooting your calling issues, password, or phone not working." ]
              },
              "languageCode": "en"
            } ]
          },
          "name": "67014c5f-143f-49aa-bf6c-8e4b311a1d28"
        }, {
          "event": "sys.no-match-2",
          "triggerFulfillment": {
            "messages": [ {
              "text": {
                "text": [ "Je ne crois toujours pas avoir compris.", "Je ne comprends toujours pas bien.", "Je ne crois toujours pas avoir bien saisi." ]
              },
              "languageCode": "fr-ca"
            }, {
              "text": {
                "text": [ "Pour obtenir de l'aide concernant les appels, dites problèmes d'appel ou faites le 1, pour les mots de passe, dites problèmes de mot de passe ou faites le 2, pour votre téléphone, dites mon téléphone ne fonctionne pas, ou faites le 3." ]
              },
              "languageCode": "fr-ca"
            }, {
              "text": {
                "text": [ "I still didn't get that.", "I still don't hear you.", "I still didn't catch that." ]
              },
              "languageCode": "en"
            }, {
              "text": {
                "text": [ "For help with Calling, say Calling issues or press 1, say Password issues, or press 2, Phone not working, or press 3." ]
