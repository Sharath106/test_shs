{
  "name": "a9f1637e-a6fd-48d7-a352-a1d43000f9dc",
  "displayName": "Phone Number Change Eligibility ",
  "form": {
    "parameters": [ {
      "displayName": "phone_number",
      "required": true,
      "entityType": "@sys.phone-number",
      "fillBehavior": {
        "initialPromptFulfillment": {
          "messages": [ {
            "text": {
              "text": [ "Please let me know which number you would like to choose. " ]
            },
            "languageCode": "en"
          } ]
        },
        "repromptEventHandlers": [ {
          "event": "sys.no-match-1",
          "triggerFulfillment": {
            "messages": [ {
              "text": {
                "text": [ "Sorry, I didn't catch that. Please select one of the five available TELUS phone numbers that have been presented to complete your phone number change. " ]
              },
              "languageCode": "en"
            } ]
          },
          "name": "cea09d5d-a370-4ac6-b667-1584e46112bd"
        }, {
          "targetFlow": "Default Start Flow",
          "event": "sys.no-match-2",
          "triggerFulfillment": {
            "messages": [ {
              "text": {
                "text": [ "I'm still not sure I got this. Let's start over, can you try rephrasing one more time?" ]
              },
              "languageCode": "en"
            } ]
          },
          "name": "9a40229a-98b6-43a2-bc78-4707d0eced1e"
        }, {
          "targetFlow": "Universal handoff",
          "event": "sys.no-match-3",
          "triggerFulfillment": {
            "messages": [ {
              "text": {
                "text": [ "Sorry, I'm still not understanding, let me transfer you to somebody else for more assistance. " ]
              },
              "languageCode": "en"
            } ]
          },
          "name": "69cb2c2d-a6b0-4ee2-ad14-05ff33f8c46f"
        } ]
      },
      "advancedSettings": {
        "dtmfSettings": {
          "useSystemEntityRule": false
        }
      }
    } ]
  },
  "entryFulfillment": {
    "messages": [ {
      "text": {
        "text": [ "[API] You are eligible to change your phone number. Here are 5 TELUS phone numbers that are available for you now (1)(2)(3)(4)(5) " ]
      },
      "languageCode": "en"
    } ]
  },
  "transitionRoutes": [ {
    "condition": "$page.params.status = FINAL",
    "triggerFulfillment": {
      "messages": [ {
        "text": {
          "text": [ "Great! You have selected $session.params.phone_number to replace 5555555555.  " ]
        },
        "languageCode": "en"
      }, {
        "text": {
          "text": [ "Please ensure that all the devices that are linked to 5555555555 are in the same local area." ]
        },
        "languageCode": "en"
      } ]
    },
    "targetPage": "Confirm Update",
    "name": "47b724f8-1fba-44da-9a62-9f48a95105ce"
  } ],
  "eventHandlers": [ {
    "event": "sys.no-match-1",
    "triggerFulfillment": {
      "messages": [ {
        "text": {
          "text": [ "Customers with a Quebec, Saskatchewan or Manitoba can only change their TELUS phone number to another TELUS phone number that is within their respective province. " ]
        },
        "languageCode": "en"
      } ]
    },
    "name": "27da6092-27e6-48de-9552-108d76f8526d"
  }, {
    "targetFlow": "Default Start Flow",
    "event": "sys.no-match-2",
    "triggerFulfillment": {
    
    },
    "name": "85e6e15f-6d30-42dc-99f5-a6ca244b7dcd"
  } ]
}
