    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.repeat_counter = null OR $session.params.repeat_counter = 3",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "At TELUS we offer a variety of plans to choose from to best suit your needs." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Whether you're in a contract or month to month, I'm sure we can find the plan that works best for you." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you are interested in bringing your mobile services over from another provider to TELUS, I would need to connect you to a team member to explore your options." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Chez TELUS nous offrons une variété de forfaits pour bien répondre à vos besoins." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Que vous ayez une entente à durée déterminée ou sur une base mensuelle, nous trouverons le forfait parfait pour vous." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si vous voulez transférer un téléphone mobile d’un autre fournisseur à TELUS, je peux vous transférer à un membre de l’equipe pour explorer vos options." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 1 OR $session.params.repeat_counter = 4",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "TELUS provides a range of plans to choose from that cater to your specific requirements." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Regardless of whether you're on a contract or a month-to-month basis, we're confident we can find the plan that best suits your needs." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you're considering switching your mobile services from another provider to TELUS, I'll need to connect you with a team member to explore your options." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "TELUS propose une gamme de forfaits parmi lesquels vous pouvez choisir en fonction de vos besoins spécifiques." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Que vous soyez sous contrat ou sur une base mensuelle, nous sommes convaincus de pouvoir trouver le forfait qui vous convient le mieux." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si vous envisagez de transférer vos services mobiles d'un autre fournisseur vers TELUS, je vais devoir vous mettre en relation avec un membre de l'équipe pour explorer vos options." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 2 OR $session.params.repeat_counter = 5",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "At TELUS, we offer a variety of plans to select from that are customized to meet your specific needs." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Whether you're on a contract or a month-to-month basis, we're certain we can find the ideal plan for you." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you're interested in transferring your mobile services from another provider to TELUS, I'll have to refer you to a team member to explore your options." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Chez TELUS, nous offrons une variété de forfaits adaptés à vos besoins spécifiques." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Que vous soyez sous contrat ou sur une base mensuelle, nous sommes certains de pouvoir trouver le forfait idéal pour vous." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si vous souhaitez transférer vos services mobiles d'un autre fournisseur vers TELUS, je vais devoir vous référer à un membre de l'équipe pour explorer vos options." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      } ]
