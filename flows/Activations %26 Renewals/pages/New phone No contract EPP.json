    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.repeat_counter = null OR $session.params.repeat_counter = 3",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "You can definitely buy a device without a new service agreement for it's regular price." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "However, a service agreement can provide additional offerings, as well as the ability to finance." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you're still interested in buying without an agreement, please visit one of our authorized retailers for more details." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Vous pouvez certainement acheter un appareil à plein prix et sans avoir à signer une nouvelle entente." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "N'oubliez pas, que nous avons toujours des bonnes offres et que vous recevez un financement additionnel sur le prix d'achat de l'appareil lorsque vous choisissez de signer un contrat." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si vous souhaitez toujours acheter un nouvel appareil à plein prix ou sans contrat, veuillez visiter l'un de nos détaillants autorisé." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Sinon, je peux vous envoyer un message texte avec plus d'informations sur les offres auxquelles votre organisation est admissible pour." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 1 OR $session.params.repeat_counter = 4",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Purchasing a device at its regular price without a new service agreement is possible." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "However, a service agreement can offer additional benefits and financing options." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you prefer to buy without an agreement, please visit one of our authorized retailers for more information." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Il est possible d'acheter un appareil au prix régulier sans nouvel accord de service." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Cependant, un accord de service peut offrir des avantages supplémentaires et des options de financement." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si vous préférez acheter sans accord, veuillez visiter l'un de nos détaillants autorisés pour plus d'informations." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Sinon, je peux vous envoyer un message texte avec plus d'informations sur les offres auxquelles votre organisation est admissible pour." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 2 OR $session.params.repeat_counter = 5",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "If you're considering buying a device without a new service agreement, it's possible to do so at its regular price." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Nevertheless, a service agreement can provide additional benefits and financing options." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you still want to buy without an agreement, please visit one of our authorized retailers for further details." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si vous envisagez d'acheter un appareil sans nouvel accord de service, il est possible de le faire au prix régulier." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Cependant, un accord de service peut offrir des avantages supplémentaires et des options de financement." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si vous souhaitez toujours acheter sans accord, veuillez visiter l'un de nos détaillants autorisés pour plus de détails." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Sinon, je peux vous envoyer un message texte avec plus d'informations sur les offres auxquelles votre organisation est admissible pour." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      } ]
