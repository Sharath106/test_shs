    "triggerFulfillment": {
      "messages": [ {
        "text": {
          "text": [ "Essayez les étapes suivantes si une application installée sur votre téléphone ne fonctionne pas." ]
        },
        "languageCode": "fr-ca"
      }, {
        "text": {
          "text": [ "Redémarrez votre téléphone.\r\nVérifiez les mises à jour Android.\r\nMettre à jour l'application." ]
        },
        "languageCode": "fr-ca"
      }, {
        "text": {
          "text": [ "Forcer l'arrêt de l'application.\nEffacez le cache et les données de l'application.\nSynchronisez l'appareil avec les serveurs Google.\nDésinstallez l'application.\nContacter le développeur de l'application" ]
        },
        "languageCode": "fr-ca"
      }, {
        "text": {
          "text": [ "Try the following steps if an app installed on your phone isn't working." ]
        },
        "languageCode": "en"
      }, {
        "text": {
          "text": [ "Restart your phone.\nCheck for Android updates.\nUpdate the app." ]
        },
        "languageCode": "en"
      }, {
        "text": {
          "text": [ "Force stop the app.\nClear the app's cache \u0026 data.\nSync device with Google servers.\nUninstall the app.\nContact the app developer" ]
