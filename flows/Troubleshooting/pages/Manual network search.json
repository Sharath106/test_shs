    "condition": "$session.params.iphone_vs_android = \"iphone\" OR $session.params.iphone_vs_android = \"1\"",
    "triggerFulfillment": {
      "messages": [ {
        "text": {
          "text": [ "In your iPhone:\n\nGo to Settings, Select Cellular and Choose Network Selection." ]
        },
        "languageCode": "en"
      }, {
        "text": {
          "text": [ "Wait for about a minute, and you should see available networks showing." ]
        },
        "languageCode": "en"
      }, {
        "text": {
          "text": [ "Select TELUS, or the roaming partner you prefer if applicable." ]
        },
        "languageCode": "en"
      }, {
        "text": {
          "text": [ "Did that work?" ]
        },
        "languageCode": "en"
      }, {
        "text": {
          "text": [ "Dans votre iPhone:\n\nAllez dans Réglages, puis Cellulaire, puis Sélection du réseau." ]
        },
        "languageCode": "fr-ca"
      }, {
        "text": {
          "text": [ "Attendez environ une minute, et vous devriez voir apparaître les réseaux disponibles." ]
        },
        "languageCode": "fr-ca"
      }, {
        "text": {
          "text": [ "Sélectionnez TELUS ou le partenaire d'itinérance que vous préférez." ]
        },
        "languageCode": "fr-ca"
      }, {
        "text": {
          "text": [ "Est-ce que ça a fonctionné?" ]
