    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.repeat_counter = null OR $session.params.repeat_counter = 3",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Si vous rencontrez des problèmes avec votre courriel, voici quelques trucs  à essayer:" ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "1, Assurez-vous que les données ou le Wi-Fi sont activés sur votre téléphone." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "2, Confirmez que vous pouvez accéder à votre messagerie via le Web." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "et 3, Supprimez et rajoutez le courriel de votre téléphone." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you're having issues with your email, here are few things to try:" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "1, Make sure that your mobile data or Wi-Fi is enabled on your phone." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "2, Confirm that you can access your email through the web and" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "3, Try to remove and re-add the email on your phone allowing it to resync itself." ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 1 OR $session.params.repeat_counter = 4",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Si vous rencontrez des problèmes avec votre courrier électronique, essayez les étapes suivantes :" ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Assurez-vous que les données mobiles ou le Wi-Fi de votre téléphone sont activés." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Vérifiez si vous pouvez accéder à votre courrier électronique via le web." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Supprimez et réajoutez le compte de messagerie sur votre téléphone, en lui permettant de se resynchroniser." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you're experiencing email problems, try the following:" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "1. Ensure that your phone's mobile data or Wi-Fi is turned on." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "2. Check if you can access your email through the web." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "3. Remove and re-add the email account on your phone, allowing it to resync." ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 2 OR $session.params.repeat_counter = 5",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Si vous rencontrez des problèmes avec votre courrier électronique, commencez par vérifier que les données mobiles ou le Wi-Fi de votre téléphone sont activés." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Ensuite, vérifiez si vous pouvez accéder à votre courrier électronique via le web." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Si le problème persiste, essayez de supprimer et de réajouter le compte de messagerie sur votre téléphone, en lui permettant de se resynchroniser." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you're encountering email issues, start by verifying that your phone's mobile data or Wi-Fi is enabled. Next, check if you can access your email through the web. If the problem persists, try removing and re-adding the email account on your phone, allowing it to resync." ]
            },
            "languageCode": "en"
          }
        } ]
