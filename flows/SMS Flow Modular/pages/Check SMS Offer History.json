{
  "name": "8d25e5b2-b44d-4b16-b877-8cd50b130109",
  "displayName": "Check SMS Offer History",
  "form": {
  
  },
  "entryFulfillment": {
    "setParameterActions": [ {
      "parameter": "file_name",
      "value": "res_wls_sms_rules.json"
    }, {
      "parameter": "business_unit",
      "value": "$sys.func.IF(\"$session.params.routeDetails.businessUnit != null\", $session.params.routeDetails.businessUnit, \"res\")"
