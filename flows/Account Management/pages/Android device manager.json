  "entryFulfillment": {
    "setParameterActions": [ {
      "parameter": "last_page",
      "value": "Android device manager"
    } ],
    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.repeat_counter = null OR $session.params.repeat_counter = 3",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "\"Find My Device\" is a feature by Google to help locate your phone." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If it's enabled, you can track your phone in case it becomes lost, or stolen." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "You'll have to use your Gmail account, and it comes with the option for your phone to ring up to 5 minutes, even on silent mode." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Localiser mon appareil est un service fourni par Google pour localiser votre téléphone." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "S'il est activé, vous pouvez suivre votre téléphone en cas de perte ou de vol." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Vous devez avoir créé un compte Gmail et vous aurez aussi la possibilité de faire sonner votre téléphone pendant un maximum de cinq minutes même si la sonnerie était coupée." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 1 OR $session.params.repeat_counter = 4",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Google's \"Find My Device\" is a tool designed to assist in locating a lost or stolen phone." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Once activated, users can track the location of their device using their associated Gmail account." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Additionally, the feature includes the option to make the phone ring for up to 5 minutes, even if it's on silent mode." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Localiser mon appareil de Google est un outil conçu pour aider à localiser un téléphone perdu ou volé." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Une fois activé, les utilisateurs peuvent suivre l'emplacement de leur appareil en utilisant leur compte Gmail associé." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "De plus, cette fonctionnalité permet de faire sonner le téléphone pendant cinq minutes, même s'il est en mode silencieux." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "$session.params.repeat_counter = 2 OR $session.params.repeat_counter = 5",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Google offers a feature called \"Find My Device\" which is intended to help users locate their lost or stolen phone." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "By enabling this feature, users can track their phone's location using their Gmail account." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Moreover, the feature allows the phone to ring for up to 5 minutes, even when it's on silent mode." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Google propose une fonctionnalité appelée Localiser mon appareil qui permet aux utilisateurs de retrouver leur téléphone perdu ou volé." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "En activant cette fonctionnalité, les utilisateurs peuvent suivre la localisation de leur téléphone en utilisant leur compte Gmail." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "De plus, cette fonctionnalité permet de faire sonner le téléphone pendant cinq minutes, même s'il est en mode silencieux." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      } ]
    } ]
