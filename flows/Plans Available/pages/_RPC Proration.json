  "entryFulfillment": {
    "messages": [ {
      "text": {
        "text": [ "\u003cspeak\u003eOn your next bill, along with the regular \u003csay-as interpret-as=\"currency\" language='en-US'\u003e$$session.params.selected_plan_details.chargeAmount\u003c/say-as\u003e monthly cost, you'll find a credit of \u003csay-as interpret-as=\"currency\" language='en-US'\u003e$$session.params.partial_credit\u003c/say-as\u003e from your old plan and a partial charge of \u003csay-as interpret-as=\"currency\" language='en-US'\u003e$$session.params.partial_charge\u003c/say-as\u003e for the new plan starting today.\u003c/speak\u003e" ]
      },
      "languageCode": "en"
    }, {
      "text": {
        "text": [ "Would you like to continue to change to this plan?" ]
      },
      "languageCode": "en"
    }, {
      "text": {
        "text": [ "\u003cspeak\u003eEn addition aux frais mensuels réguliers de \u003csay-as interpret-as=\"currency\" language='fr-ca'\u003e$$session.params.selected_plan_details.chargeAmount\u003c/say-as\u003e, vous verrez un crédit de \u003csay-as interpret-as=\"currency\" language='fr-ca'\u003e$$session.params.partial_credit\u003c/say-as\u003e pour votre ancien forfait et des frais partiels de \u003csay-as interpret-as=\"currency\" language='fr-ca'\u003e$$session.params.partial_charge\u003c/say-as\u003e pour le nouveau forfait débutant aujourd'hui.\u003c/speak\u003e" ]
      },
      "languageCode": "fr-ca"
    }, {
      "text": {
        "text": [ "Voulez-vous continuer et modifier votre forfait?" ]
      },
      "languageCode": "fr-ca"
    } ],
