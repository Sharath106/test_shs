  "entryFulfillment": {
    "messages": [ {
      "text": {
        "text": [ "Bien sûr, comment puis-je vous aider avec votre facture aujourd'hui ?" ]
      },
      "languageCode": "fr"
    } ],
    "setParameterActions": [ {
      "parameter": "last_page",
      "value": "BillDisambiguation"
    } ],
    "conditionalCases": [ {
      "cases": [ {
        "condition": "$session.params.bill_informational = true",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Unfortunately, I can't verify your account without your pin, but you can still ask me other things like 'pay my bill,' 'change my plan,' or 'roaming charges'." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Malheureusement, je ne peux pas valider votre compte sans votre NIP, mais vous pouvez dire des choses comme \"payer ma facture\", \"changer de forfait\" ou \"frais d'itinérance\"." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "($session.params.unauth = true OR $session.params.billvague_billsum = true) OR $session.params.chargelist = null OR $session.params.getbillinfoerror = true",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "How can I help with your bill ? you can say things such as, \"my bill is too high\",, \"explain my bill\",,or, \"missing a credit or promotion\"." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Comment puis-je vous aider avec votre facture? Vous pouvez dire des choses telles que ma facture est trop élevée\",, \"expliquer ma facture\",,ou, \"crédit ou promotion manquants\"." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "$session.params.bill_summary[0] != null AND $session.params.service = null AND $session.params.billvague_billexpattempt = true",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "How can I help with your bill ? you can say things such as, \"my bill is too high\",, \"missing a credit or promotion\",,or, aks about any other billing related questions." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Comment puis-je vous aider avec votre facture? Vous pouvez dire des choses telles que, \"ma facture est trop élevée\",, \"crédit ou promotion manquants\",,ou, pour toute autre question relative à la facturation." ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      }, {
        "condition": "$session.params.bill_summary[0] != null AND $session.params.service = null",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Would you like to know more about any of these service charges ?" ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Souhaitez-vous en savoir plus sur ces frais de service?" ]
            },
            "languageCode": "fr-ca"
          }
        } ]
      } ]
    } ]
