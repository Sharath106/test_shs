    "condition": "$session.params.data_settings = \"match\" OR $session.params.data_settings = \"2\"",
    "triggerFulfillment": {
      "messages": [ {
        "text": {
          "text": [ "Pour correspondre à votre cycle de facturation:\nOuvrez les paramètres de votre téléphone, sélectionnez Réseau et Internet ou Réseaux mobiles ou Connexions.\nAppuyez sur Données ou Données cellulaires et sélectionnez Avertissements et limites de données." ]
        },
        "languageCode": "fr-ca"
      }, {
        "text": {
          "text": [ "Ensuite, sélectionnez l'option Cycle d'util. données d'appli, puis définissez votre date de réinitialisation du cycle de consommation." ]
        },
        "languageCode": "fr-ca"
      }, {
        "text": {
          "text": [ "To Match your bill cycle:\nIn Settings of your device, Select Network \u0026 Internet, or Mobile Network, or Connections.\nThen, Select Data Usage or More Data Settings, then Select Billing cycle and data warning, or More data setting." ]
        },
        "languageCode": "en"
      }, {
        "text": {
          "text": [ "After that, Select Start billing cycle on, or Start date, then select the beginning of your bill cycle and hit OK." ]
        },
        "languageCode": "en"
      } ],
