    "conditionalCases": [ {
      "cases": [ {
        "condition": "($session.params.repeat_counter = null OR $session.params.repeat_counter = 3) AND $session.params.iphone_vs_android = null",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "La mention 4G, 5G, ou, L T E apparaissant dans le haut de l’écran indique que vous êtes connecté aux données cellulaires." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Lorsque vous êtes connecté à un réseau Wi-Fi, une icône ressemblant à une pointe de tarte s’affiche dans le haut de l’écran." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "La meilleure façon de s’assurer qu’aucune donnée n’est utilisée est de désactiver la transmission de données." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you see 4G, 5G, or, LTE at the very top of your screen, this indicates you're connected to cellular data." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "When you're connected to Wi-Fi, you'll instead see an icon that resembles a slice of pie." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "The safest way to ensure you're not using cellular data is by disabling it in your settings, as some apps may run or update in the background." ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "condition": "($session.params.repeat_counter = 1 OR $session.params.repeat_counter = 4) AND $session.params.iphone_vs_android = null",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Lorsque vous voyez 4G, 5G ou LTE en haut de votre écran, cela signifie que vous êtes connecté aux données cellulaires." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "En revanche, lorsque vous êtes connecté au Wi-Fi, une icône ressemblant à une part de tarte apparaîtra à la place." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Pour éviter d'utiliser les données cellulaires sans le savoir, il est préférable de les désactiver dans vos paramètres, car certaines applications peuvent continuer à s'exécuter ou à se mettre à jour en arrière-plan." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "When you see 4G, 5G, or LTE at the top of your screen, it means that you're connected to cellular data." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "On the other hand, when you're connected to Wi-Fi, an icon resembling a slice of pie will appear instead." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "To avoid using cellular data unknowingly, it's best to disable it in your settings, as some apps may continue to run or update in the background." ]
            },
            "languageCode": "en"
          }
        } ]
      }, {
        "condition": "($session.params.repeat_counter = 2 OR $session.params.repeat_counter = 5) AND $session.params.iphone_vs_android = null",
        "caseContent": [ {
          "message": {
            "text": {
              "text": [ "Si vous remarquez 4G, 5G ou LTE en haut de votre écran, cela signifie que vous utilisez des données cellulaires." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Cependant, si vous êtes connecté au Wi-Fi, vous verrez une icône qui ressemble à une part de tarte à la place." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "Pour éviter d'utiliser les données cellulaires involontairement, il est préférable de les désactiver dans vos paramètres, car certaines applications peuvent continuer à fonctionner ou à se mettre à jour en arrière-plan." ]
            },
            "languageCode": "fr-ca"
          }
        }, {
          "message": {
            "text": {
              "text": [ "If you notice 4G, 5G, or LTE at the top of your screen, it means that you're using cellular data." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "However, if you're connected to Wi-Fi, you'll see an icon that looks like a slice of pie instead." ]
            },
            "languageCode": "en"
          }
        }, {
          "message": {
            "text": {
              "text": [ "To prevent using cellular data unintentionally, it's best to turn it off in your settings, as some apps may continue to operate or update in the background." ]
            },
            "languageCode": "en"
          }
        } ]
      } ]
    } ]
